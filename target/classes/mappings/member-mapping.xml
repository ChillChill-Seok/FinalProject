<insert id="insertMember">
	INSERT INTO MEMBER 
	VALUES( #{ userId }, #{ userPwd }, #{ userName }, #{nickName}, #{ birthDay }, 
	                         #{ gender }, #{ email }, #{ phone }, 
	                         SYSDATE, SYSDATE, 'Y')
</insert>

<select id="duplicateCheck" parameterType="String" resultType="integer">
	SELECT COUNT(*) FROM MEMBER
	WHERE ID = #{ userId }
</select>

	
	
</mapper>