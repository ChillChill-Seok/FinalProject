<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="MyMusicDAO">
    <resultMap id="myMusicResult" type="myMusic">
        <id column="id" property="id"/>
        <id column="list_num" property="listNum"/>
        <result column="article_no" property="articleNo"/>
    </resultMap>
    <select id="selectMyMusicList" resultMap="myMusicResult">
        select * from my_music where id = #{id}
    </select>
    <select id="getMyMusicMaximum" resultType="int">
        select nvl(max(list_num), 0) from my_music where id = #{id}
    </select>
    <insert id="insertMyMusic">
        insert into my_music values(#{listNum}, #{id}, #{articleNo})
    </insert>
</mapper>