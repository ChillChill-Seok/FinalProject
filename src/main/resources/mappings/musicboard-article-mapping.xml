<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="MusicBoardArticleDAO">
    <resultMap id="articleResult" type="musicBoardArticle">
        <id column="article_no" property="article_no"/>
        <result column="id" property="id"/>
        <result column="title" property="title"/>
        <result column="text" property="text"/>
        <result column="file_path" property="file_path"/>
    </resultMap>
    <insert id="insertArticle">
    insert into musicboard_article
    values(mb_article_seq.nextval, #{id}, #{title}, #{text}, #{fileName})
  </insert>
</mapper>